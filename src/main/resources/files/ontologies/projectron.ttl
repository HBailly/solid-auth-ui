<>
    dcterms:issued   "2022-06-22"^^xsd:date ;
    dcterms:modified "2020-06-22"^^xsd:date ;
    rdfs:label       "Terms for Projectron key resources"@en .

#############################################################################

:Project
    a                rdfs:Class ;
    rdfs:comment     "A  planned piece of work that has a particular aim, especially one that is organized by a government, company, or other organization."@en ;
    rdfs:isDefinedBy <> ;
    rdfs:label       "Project"@en .

:Task
    a                rdfs:Class ;
    rdfs:comment     "A usually assigned piece of work often to be finished within a certain time."@en ;
    rdfs:isDefinedBy <> ;
    rdfs:label       "Task"@en .

:Contact
    a                rdfs:Class ;
    rdfs:comment     "An individual human, usually an adult."@en ;
    rdfs:isDefinedBy <> ;
    rdfs:label       "Contact"@en .
